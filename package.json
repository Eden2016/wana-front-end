{
  "name": "wana-frontend",
  "version": "0.0.1",
  "license": "MIT",
  "main": "index.js",
  "description": "Frontend for the Wana web app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/powerhousecreative/wana-frontend.git"
  },
  "author": "BEAM & Powerhouse Creative",
  "bugs": {
    "url": "https://github.com/powerhousecreative/wana-frontend/issues"
  },
  "homepage": "https://github.com/powerhousecreative/wana-frontend",
  "scripts": {
    "start": "webpack-dev-server",
    "client:dev": "webpack-dashboard -t 'Marvin' -- webpack-dev-server",
    "client:watch": "webpack",
    "client:build": "npm run clean && cross-env NODE_ENV=\"development\" webpack",
    "client:deploy": "npm run clean && cross-env NODE_ENV=\"development\" webpack --config webpack.config.deploy.js",
    "client:preview": "npm run clean && cross-env NODE_ENV=\"production\" webpack-dev-server",
    "server:watch": "cross-env SERVER_RENDER=\"true\" webpack --config webpack.config.server.js",
    "server:restart": "nodemon ./build/server.js",
    "server:build": "cross-env SERVER_RENDER=\"true\" cross-env NODE_ENV=\"production\" webpack --config webpack.config.server.js",
    "server:dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"webpack-server,watch-server\" --kill-others \"npm run server:watch\" \"npm run server:restart\"",
    "universal:dev": "concurrently --prefix \"[{name}]\" --names \"webpack-server,webpack-client,watch-server\" --kill-others \"npm run server:watch\" \"npm run client:watch\" \"npm run server:restart\"",
    "universal:build": "npm run client:build && npm run server:build",
    "clean": "rm -rf ./build",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prepush": "^3.1.11",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-svg-loader": "^1.1.1",
    "redux-logger": "^3.0.6",
